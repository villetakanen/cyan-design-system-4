{
  "name": "@11thdeg/cn-story-clock",
  "version": "4.0.0-beta.5",
  "description": "A form input custom element for a PbtA style story clock",
  "type": "module",
  "main": "dist/cn-story-clock.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cn-story-clock.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
