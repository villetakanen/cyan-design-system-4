  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@vitest/browser": "^3.2.4",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
  "typescript": "^5.8.3",
  "vite": "^6.0.7",
    "vitest": "^3.2.4",
    "vitest-browser-lit": "^0.1.0"
  },
  "dependencies": {
    "lit": "^3.3.1",
    "ts-key-enum": "^3.0.13"
  }
