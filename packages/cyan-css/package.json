{
  "name": "@11thdeg/cyan-css",
  "version": "4.0.0-beta.16",
  "description": "Cyan Design System CSS Styles",
  "type": "module",
  "main": "src/index.css",
  "style": "src/index.css",
  "exports": {
    ".": "./src/index.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "pnpm build",
    "build": "cpx 'src/**/*.css' dist",
    "dev": "cpx 'src/**/*.css' dist --watch"
  },
  "keywords": [
    "css",
    "design-system",
    "styles"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@fontsource/open-sans": "*",
    "lato-font": "*"
  },
  "devDependencies": {
    "cpx2": "^8.0.0"
  }
}
